{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build:ui": "@powershell Remove-Item -Recurse -Force build && cd ../FrontEnd/ && npm run build && @powershell Copy-Item ./build -Recurse ../BackEnd/",
    "deploy": "git push heroku master",
